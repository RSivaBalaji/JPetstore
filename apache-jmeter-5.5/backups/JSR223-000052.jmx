            <stringProp name="parameters"></stringProp>
            <stringProp name="script">//Example: 1
log.info(&quot;*************1sr Example for JSR223 PreProcessor*******************&quot;);
log.info(&quot;Little Law Information&quot;);
log.error(&quot;Incorrect Law Order&quot;);
log.warn(&quot;Little Law Warning&quot;);

//Example: 2
log.info(&quot;*************2nd Example for JSR223 PreProcessor*******************&quot;);
def text = &quot;Little&apos;s Law Groovy&quot;;
log.info(text);
log.info(&quot;The Example Two is Combining with the Text: &quot;+text);

//Example: 3
log.info(&quot;*************3rd Example for JSR223 PreProcessor*******************&quot;);
// Get the variable from JMeter (CSV data)
def my_var=vars.get(&quot;Name&quot;);

// Log the value of the variable
log.info(&quot;The Value of my my_var is: &quot; + my_var);

//Example: 4
log.info(&quot;*************4th Example for JSR223 PreProcessor*******************&quot;);
vars.put(&quot;MY_VAR&quot;,&quot;ERVINLAW&quot;);

//Example: 5
log.info(&quot;*************5th Example for JSR223 PreProcessor*******************&quot;);
def text06=&quot;example6&quot;;
vars.put(&quot;My_text&quot;,text06);

//Example: 6
log.info(&quot;........................6th example for JSR22 Preprocessor........................&quot;);

def my_number_06=vars.get(&quot;Name&quot;).toInteger();

def new_number=3;

def add = my_number_06+new_number;
def sub = my_number_06-new_number;
def mul = my_number_06*new_number;

vars.put(&quot;My_Number_06&quot;,add.toString());
vars.put(&quot;Subtraction&quot;, sub.toString());
vars.put(&quot;Multiplication&quot;, mul.toString());

</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
          </JSR223PreProcessor>
